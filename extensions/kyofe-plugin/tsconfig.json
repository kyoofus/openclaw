{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
    "paths": {
      "openclaw/plugin-sdk": ["./node_modules/openclaw/dist/plugin-sdk"]
    }
  },
  "include": ["*.ts"],
  "exclude": ["node_modules", "dist"]
}
